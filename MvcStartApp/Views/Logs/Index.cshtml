@model MvcStartApp.Models.Db.Request[];

@{
    ViewData["Title"] = "Журнал событий";
}

<style>
    table {
        border-spacing: 0 15px;
        align-self: center;
    }

    th {
        background-color: #4287f5;
        color: white;
    }

    th, td {
        width: 150px;
        text-align: center;
        border: 1px solid black;
        padding: 5px;
    }

    h2 {
        color: #4287f5;
    }
</style>

<h1>
    Журнал событий
</h1>

<div class="logs-container">
    <table>
        <tr>
            <td>Дата</td>
            <td>URL запроса</td>
        </tr>
        @foreach(var request in Model)
        {
            <tr>
                <td>@request.Date</td>
                <td>@request.Url</td>
            </tr>
        }
    </table>
</div>